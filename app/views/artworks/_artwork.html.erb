<%= turbo_frame_tag "artwork_#{artwork.id}" do %>
  <div id="<%= dom_id artwork %>">
    <tr>
      <td><%= artwork.id %></td>
      <td><%= link_to artwork.title, artwork %></td>
      <td><%= artwork.medium %></td>
      <td><%= artwork.description %></td>
      <td><%= artwork.visible %></td>
      <td>
        <%= button_to "Delete", artwork, method: :delete, class: "btn btn-danger", form: { data: { turbo_confirm: 'Are you sure?' } }, class: "btn btn-danger mx-1" %>
      </td>
    </tr>
  </div>
<% end %>
