<div class="col-4">
  <a class="d-block" href="">
    <%= image_tag large_url(i), alt: i.caption, class: "img-fluid" %>
  </a>
  <%= i.caption %>
  <%= button_to asset_delete_text(i), [@artwork, i], method: :delete, class: "btn btn-danger btn-sm mt-2 mb-4", form: { data: { turbo_confirm: 'Are you sure?' } } %>
</div>
